{
  "id": "external-haproxy",
  "instances": 1,
  "cpus": 0.2,
  "mem": 256,
  "healthChecks": [ { "path": "/haproxy_stats", "port": 9090 } ],
  "constraints": [["hostname","CLUSTER","172.16.1.23"]],
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "nelsd/external_proxy",
      "network": "HOST"
    }
  },
  "env": {
    "REFRESH_DELAY": "5"
  }
}

