{
  "id": "dots_frontend",
  "cpus": 0.1,
  "mem": 64.0,
  "instances": 4,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "nelsd/dots_front:latest",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 80, "hostPort": 0, "protocol": "tcp" }
      ]
    }
  },
  "healthChecks": [
    {
      "protocol": "HTTP",
      "portIndex": 0,
      "path": "/",
      "gracePeriodSeconds": 5,
      "intervalSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ],
  "labels":{
    "HAPROXY_GROUP":"external"
  }
}
